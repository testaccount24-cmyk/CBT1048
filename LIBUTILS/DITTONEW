         DC    C'TTR '                                                  FT019900
         DC    AL4(0)                  NO BR IN THIS MODULE             FT020000
         DC    C'LCU '                 LISTCTLG                         FT020100
         B     LST                                                      FT020200
         DC    C'LPU '                 LISTPDS                          FT020300
