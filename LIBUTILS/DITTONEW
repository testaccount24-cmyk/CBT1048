MOVLOPS  LA    R8,SCARG                PTR TO OUTPUT                    CN049700
MOVLOP   CLC   QUOTES(2),0(RC)         '' ?                             CN049800
         BE    MOVLOP1                 YES                              CN049900
         CLC   QUOTES(1),0(RC)         ' ?                              CN050000
