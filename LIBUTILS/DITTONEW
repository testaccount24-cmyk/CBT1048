         DC    F'0'                                                     LO320800
         DC    H'1'                                                     LO320900
         DC    H'0'                                                     LO321000
OUTDASD  DC    X'00'                                                    LO321100
OUTSEEK  DC    XL7'00'                                                  LO321200
         SPACE 1                                                        LO321300
CVTPTR   EQU   16                                                       LO321400
R0       EQU   0                                                        LO321500
R1       EQU   1                                                        LO321600
R2       EQU   2                                                        LO321700
