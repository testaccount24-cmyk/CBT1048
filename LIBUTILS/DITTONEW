         DC    X'02'                                                    LO318300
         DC    AL3(OUTIOVEC)                                            LO318400
OTDEBUCB EQU   *                                                        LO318500
OUTDEBMD DC    X'00'                                                    LO318600
OUTDBUCB DC    X'000000'                                                LO318700
         DC    F'0'                                                     LO318800
         SPACE 1                                                        LO318900
