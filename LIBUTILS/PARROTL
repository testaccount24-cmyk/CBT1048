          LITERAL_POOL(40) CHAR(20),                                       06410
          ZSW_GR11(60) BIT(1) INIT((60)(1)'1'B),                           06420
          ZSW BIT(1) INIT('1'B),                                           06430
          SW11 BIT(1),                                                     06440
          SW12 BIT(1),                                                     06450
          NO_FMAT_LITS INIT(0),                                            06460
          NO_GR10      INIT(0),                                            06470
