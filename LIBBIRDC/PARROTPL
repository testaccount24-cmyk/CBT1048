*                                                                       PL067900
*         LEXIT LABEL,                                                  PL068000
*         PEXIT LABEL,                                                  PL068100
*         REXIT LABEL;                                                  PL068200
*                                                                       PL068300
*         P = ADDR(ZREC);                                               PL068400
*                                       /* CONVERT THE IBM DATE     */  PL068500
*                                       /* INTO REPORT FORMAT       */  PL068600
*         XDATE  = DATE;                                                PL068700
*         YYEAR  = XYEAR;                                               PL068800
*         YDAY   = XDAY;                                                PL068900
*         YMONTH = ZMONTH(XMONTH);                                      PL069000
