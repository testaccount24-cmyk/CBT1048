*         IF FLD_GR10 = 'KEY6 ' THEN I = 1006;                          PL132500
*         IF FLD_GR10 = 'KEY7 ' THEN I = 1007;                          PL132600
*         IF FLD_GR10 = 'KEY8 ' THEN I = 1008;                          PL132700
*         IF FLD_GR10 = 'LINE+' THEN I = 1009;                          PL132800
*         IF I ^= 0                                                     PL132900
*         THEN DO;                                                      PL133000
