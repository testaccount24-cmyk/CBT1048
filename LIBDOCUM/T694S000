//T665F3    DD DCB=(RECFM=FB,LRECL=76,BLKSIZE=760),
//             UNIT=SYSDA,SPACE=(TRK,(38,38),RLSE)
//T665F4    DD SYSOUT=A,DCB=BLKSIZE=133
