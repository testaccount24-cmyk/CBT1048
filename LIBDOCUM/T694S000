./ ADD NAME=IMPAR807,LIST=ALL
//IMPAR807    PROC REP='006',DAT='000'
//STEP01   EXEC PGM=IERRCO00,REGION=0130K
//SYSIN DD DSN=IMPAIR.CONTRLIB(R&REP.SRT1),DISP=SHR
//SORTLIB  DD   DSN=SYS1.SORTLIB,DISP=SHR
//SYSOUT   DD   SYSOUT=A
//SORTWK01 DD   UNIT=SYSDA,SPACE=(CYL,10,RLSE,CONTIG)
//SORTWK02 DD   UNIT=SYSDA,SPACE=(CYL,10,RLSE,CONTIG)
//SORTWK03 DD   UNIT=SYSDA,SPACE=(CYL,10,RLSE,CONTIG)
//SORTIN   DD DSN=IMPAIR.T694F562(0),DISP=SHR
//       DD DSN=IMPAIR.T694F701,DISP=SHR
//       DD DSN=IMPAIR.T694F824,DISP=SHR
//SORTOUT DD DSN=&&STEP02A,DISP=(,PASS),SPACE=(CYL,(10,2),RLSE),
//     UNIT=SYSDA,DCB=(RECFM=FB,LRECL=600,BLKSIZE=2400)
//STEP03  EXEC PGM=T618P102,PARM='MONTHNO PLEASE'
//STEPLIB      DD  DSN=COMPARE.PROGLIB,DISP=SHR
//INPTDATA  DD DSN=COMPARE.CONTRLIB(MMYYPARS),DISP=SHR
//DATEFILE  DD DSN=IMPAIR.T694F&DAT.,DISP=SHR
//OUTDATA   DD DUMMY,DCB=BLKSIZE=80
//STEP05    EXEC PGM=COPY,COND=(01,NE,STEP03)
//SYSUT1  DD DSN=IMPAIR.CONTRLIB(R&REP.HDAL),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.HD01),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.LBAL),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.AP01),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.RTAL),DISP=SHR
//SYSUT2 DD DSN=&&STEP03A,DISP=(,PASS),UNIT=SYSDA,
//    DCB=(RECFM=FB,LRECL=80,BLKSIZE=80),
//     SPACE=(TRK,(9,9),RLSE)
//SYSIN DD DUMMY
//SYSPRINT DD SYSOUT=A
//STEP07    EXEC PGM=COPY,COND=(02,NE,STEP03)
//SYSUT1  DD DSN=IMPAIR.CONTRLIB(R&REP.HDAL),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.HD02),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.LBAL),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.AP02),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.RTAL),DISP=SHR
//SYSUT2 DD DSN=&&STEP03A,DISP=(,PASS),UNIT=SYSDA,
//    DCB=(RECFM=FB,LRECL=80,BLKSIZE=80),
//     SPACE=(TRK,(9,9),RLSE)
//SYSIN DD DUMMY
//SYSPRINT DD SYSOUT=A
//STEP09    EXEC PGM=COPY,COND=(03,NE,STEP03)
//SYSUT1  DD DSN=IMPAIR.CONTRLIB(R&REP.HDAL),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.HD03),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.LBAL),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.AP03),DISP=SHR
//        DD DSN=IMPAIR.CONTRLIB(R&REP.RTAL),DISP=SHR
//SYSUT2 DD DSN=&&STEP03A,DISP=(,PASS),UNIT=SYSDA,
//    DCB=(RECFM=FB,LRECL=80,BLKSIZE=80),
//     SPACE=(TRK,(9,9),RLSE)
//SYSIN DD DUMMY
//SYSPRINT DD SYSOUT=A
//STEP11    EXEC PGM=COPY,COND=(04,NE,STEP03)
//SYSUT1  DD DSN=IMPAIR.CONTRLIB(R&REP.HDAL),DISP=SHR
