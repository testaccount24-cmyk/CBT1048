//STEP09       EXEC  PGM=VAMPIRE,PARM='  '
//STEPLIB      DD  DSN=COMPARE.PROGLIB,DISP=SHR
//SYSIN  DD DSN=IMPAIR.CONTRLIB(R107VAMP),DISP=SHR
//PRINTOUT     DD  &VAMPOUT,DCB=BLKSIZE=133
