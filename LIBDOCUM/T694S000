//SYSIN        DD   DUMMY
//SYSUT1       DD   DSN=IMPAIR.T694F&FILE1,DISP=SHR
//SYSUT2       DD   DSN=IMPAIR.T694F890,DISP=(,CATLG,DELETE),
//             VOL=REF=IMPAIR.DISK1,SPACE=(TRK,(200,10)),
//             LABEL=RETPD=25,DCB=(RECFM=FB,LRECL=600,BLKSIZE=2400)
//STEP03       EXEC PGM=COPY
//SYSPRINT     DD   SYSOUT=A
//SYSIN        DD   DUMMY
//SYSUT1       DD   DSN=IMPAIR.T694F&FILE2,DISP=SHR
//SYSUT2       DD   DSN=IMPAIR.T694F890,DISP=MOD,
//             VOL=REF=IMPAIR.DISK1
//STEP04       EXEC PGM=COPY
//SYSPRINT     DD   SYSOUT=A
//SYSIN        DD   DUMMY
//SYSUT1       DD   DSN=IMPAIR.T694F&FILE3,DISP=SHR
//SYSUT2       DD   DSN=IMPAIR.T694F890,DISP=MOD,
