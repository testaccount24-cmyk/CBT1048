//STEP05       EXEC  PGM=VAMPIRE,PARM='  '
//STEPLIB      DD  DSN=COMPARE.PROGLIB,DISP=SHR
//SYSIN  DD DSN=IMPAIR.CONTRLIB(R103VAMP),DISP=SHR
//PRINTOUT     DD  SYSOUT=A,DCB=BLKSIZE=133
//INPTDATA     DD  DSN=&&STEP02C,DISP=(OLD,DELETE)
