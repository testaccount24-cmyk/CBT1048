         MACRO
         LEGEND &DATE
         DC    C'1(------------------------------)      '
#DATE    DC    CL11'&DATE'
         DC    CL3' '
         DC    C'(-----------------------------------)             '
         DC    C'(----------------------------)'
         DC    C' ( *     = SHORT WEEK           )                    '
         DC    C'(    -XX-   =   SUPPORTING          )             '
         DC    C'(    .XX.   =   ATTENDING    )'
         DC    C' ( // = PART WEEK HOLIDAY       )                    '
         DC    C'(    (XX)   =   BACKUP              )             '
         DC    C'(    +XX+   =   ATTAINING    )'
         DC    C' (////= FULL WEEK HOLIDAY       )                    '
         DC    C'(                                   )             '
         DC    C'(    *XX*   =   RESPONSIBLE  )'
         DC    C' (------------------------------)                    '
         DC    C'(-----------------------------------)             '
         DC    C'(----------------------------)'
         MEND
