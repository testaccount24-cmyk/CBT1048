//IGC00020 JOB (5806,5),'** FOR J. NOLS **',CLASS=N
//STEP1    EXEC SUPERZAP,LIB1='SYS1.LPALIB,VOL=REF=IPOVOL'
 DUMPT IGC00020 IFG0202H
 NAME  IGC00020 IFG0202H
* BECAUSE THE OLD ACCOUTING ROUTINE USES THE BLOCK-SIZE FROM
*  THE DCB TO COMPUTE THE I/O TIME, THIS ZAP WILL TEMPORARY
*  PUT THE DCBBLKSI INTO SMF14/15. BY USING THIS BLKSIZE IN
*  'CALCULATE I/O TIME' SUBROUTINE OF THE NEW ACCT.RTN THE I/O
*  TIME MUST BE THE SAME AS THE OLD ONE.
 VER   0146 D20070F92030   MVC   SMFDCBFL,DCBOFLGS
 REP   0146 D20170EC203E   MVC   SMFJFCB1+JFCBSBNM-JFCB(2),DCBBLKSI
