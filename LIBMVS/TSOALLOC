//TSOSRC  JOB (1330,5),'ALLOC.TSO SPACE',
//         CLASS=N,MSGCLASS=A,MSGLEVEL=(1,1)
//*      JOBSTREAM TO SET UP TSO DATASETS FROM NEW IPO.
//*      EACH NEW RELEASE CHANGE THE STRING 'IPO20' TO THE
//*      REQUIRED INDEX.
//DELETE       EXEC PGM=IEFBR14
//*      TO BYPASS DELETION OF OLD DATASETS CHANGE THE SYSIN
//*      CARD WHICH FOLLOWS TO BE A JCL CARD INSTEAD OF A COMMENT.
//* SYSIN      DD   DATA
//SOURCE       DD   DSN=TSG.TSO.SOURCE,DISP=(OLD,DELETE)
//HELP         DD   DSN=TSG.TSO.HELP,DISP=(OLD,DELETE)
//CMDPROC      DD   DSN=TSG.TSO.CMDPROC,DISP=(OLD,DELETE)
/*
//ALLOCATE     EXEC PGM=IEFBR14
//*      TO BYPASS ALLOCATION OF NEW DATASETS CHANGE THE SYSIN
//*      CARD WHICH FOLLOWS TO BE A JCL CARD INSTEAD OF A COMMENT.
//* SYSIN      DD   DATA
//SOURCE       DD   DSN=TSG.TSO.SOURCE,DISP=(NEW,CATLG,DELETE),
//             UNIT=DISK,VOL=REF=TSG.SOURCE,DCB=TSG.SOURCE,
//             SPACE=(CYL,(2,5,10))
//HELP         DD   DSN=TSG.TSO.HELP,DISP=(NEW,CATLG,DELETE),
//             UNIT=DISK,VOL=REF=SYS1.LINKLIB2,DCB=SYS1.HELP,
//             LABEL=EXPDT=11111,SPACE=(CYL,(5,2,15))
//CMDPROC      DD   DSN=TSG.TSO.CMDPROC,DISP=(NEW,CATLG,DELETE),
//             UNIT=DISK,VOL=REF=SYS1.LINKLIB2,DCB=IPO20.CMDPROC,
//             LABEL=EXPDT=11111,SPACE=(CYL,(2,2,15))
/*
//*      NOW COPY SOME IPO STUFF.
//COPY         EXEC PGM=IEBCOPY,REGION=256K
//SYSPRINT     DD   SYSOUT=A
//SYSUT3       DD   UNIT=SYSDA,SPACE=(CYL,(5,2))
//SYSUT4       DD   UNIT=SYSDA,SPACE=(CYL,(5,2))
//IPOCMD       DD   DSN=IPO20.CMDPROC,DISP=SHR
//TSGCMD       DD   DSN=TSG.TSO.CMDPROC,DISP=OLD
//SYSIN        DD   *
 C O=TSGCMD,I=IPOCMD
/*
//*      RENAME THE VERSION OF IEDQTCAM IN SYS1.LINKLIB
//*      SO THAT WE USE THE SYS1.LINKLIB2 VERSION,BUT CAN
//*      GO BACK IF NECESSARY.
//RENAME       EXEC PGM=MONPROGM
//SYSPRINT     DD   SYSOUT=A
//SYSUT6       DD   UNIT=SYSDA,SPACE=(TRK,(2,2))
//DISK         DD   DSN=SYS1.LINKLIB,DISP=SHR
//SYSIN        DD   *
 RENAME DSNAME=SYS1.LINKLIB,MEMBER=IEDQTCAM,NEWNAME=TSGQTCAM,          C
               VOL=DISK=IPOR01
/*
