         CPU              /* DEFAULT   COLLECT CPU STATISTICS        */
         PAGING           /* DEFAULT   COLLECT PAGING STATISTICS     */
         CHAN             /* DEFAULT   COLLECT CHANNEL STATISTICS    */
         WKLD(PERIOD)     /* DEFAULT   COLLECT WORKLOAD MANAGER
                                       STATISTICS AND REPORT AT THE
                                       PERIOD LEVEL                  */
         DEVICE(NOUNITR)  /* CHOSEN    DO NOT COLLEST UNIT RECORD
                                       STATISTICS                    */
         DEVICE(NOTAPE)   /* CHOSEN    DO NOT COLLECT TAPE DEVICE
                                       STATISTICS                    */
         DEVICE(DASD)     /* DEFAULT   COLLECT DIRECT ACCESS DEVICE
                                       STATISTICS                    */
         DEVICE(NOCOMM)   /* CHOSEN    DO NOT COLLECT COMMUNICATION
                                       EQUIPMENT STATISTICS          */
         DEVICE(NOGRAPH)  /* CHOSEN    DO NOT COLLECT GRAPHICS DEVICE
                                       STATISTICS                    */
         DEVICE(NOCHRDR)  /* CHOSEN    DO NOT COLLECT CHARACTER READER
                                       DEVICE STATISTICS             */
         REPORT(DEFER)    /* DEFAULT   PRINT REPORTS AFTER MF/1
                                       TERMINATION                   */
         SYSOUT(A)        /* DEFAULT   REPORT IN SYSOUT CLASS A      */
         RECORD           /* DEFAULT   RECORD ON SMF DATA SET        */
         INTERVAL(15M)    /* DEFAULT   REPORT AT 15 MINUTE INTERVALS */
         CYCLE(250)       /* DEFAULT   SAMPLE AT 250 MILLISECOND
                                       INTERVALS                     */
         STOP(15M)        /* DEFAULT   STOP IN 15 MINUTES            */
         OPTIONS          /* DEFAULT   ALLOW OPERATOR TO EXAMINE AND/
