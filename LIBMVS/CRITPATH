//STEP99       EXEC RELEASE,JOB=DAMCONV3
//DAMCONV3 JOB (1580,5),'0013 SETUP FOR ST.L',CLASS=N,TYPRUN=HOLD
//STEP01       EXEC PLIXCLG
//*
//*      COMPILE REFORMATING PROGRAM AND REFORMAT DATA
//*
//STEPLIB      DD   DSN=SYS2.PLIBASE,DISP=SHR
//PLI.SYSIN   DD   *
 FRIG: PROC OPTIONS (MAIN);
         DCL
         IN  FILE RECORD SEQUENTIAL,
         OUT FILE RECORD SEQUENTIAL,
         INREC CHAR(80),
         P1      DEF INREC POS(01) CHAR(01),
         P4      DEF INREC POS(04) CHAR(01),
         INODE1  DEF INREC POS(01) CHAR(04),
         INODE2  DEF INREC POS(57) CHAR(04),
         INODE3  DEF INREC POS(61) CHAR(04),
         INODE4  DEF INREC POS(69) CHAR(04),
         IDUR    DEF INREC POS(06) CHAR(03),
         IMAN    DEF INREC POS(09) CHAR(03),
         ITYPE   DEF INREC POS(18) CHAR(08),
         IDESC   DEF INREC POS(26) CHAR(24),
         ISTAT   DEF INREC POS(65) CHAR(04),
         OUTREC CHAR(80),
         ONODE1  DEF OUTREC POS(01) CHAR(04),
         ONODE2  DEF OUTREC POS(05) CHAR(04),
         ODUR    DEF OUTREC POS(10) CHAR(03),
         OTYPE   DEF OUTREC POS(40) CHAR(08),
         ODESC   DEF OUTREC POS(48) CHAR(23),
         OMAN    DEF OUTREC POS(72) CHAR(03),
         OSTAT   DEF OUTREC POS(77) CHAR(04);
         ON ENDFILE(IN) GO TO END;
         OPEN FILE(IN) INPUT;
         OPEN FILE(OUT) OUTPUT;
 READ:   READ FILE(IN) INTO(INREC);
         IF P1 = '*'
         THEN GO TO READ;
         IF P4 = '*'
         THEN GO TO READ;
         IF IDUR = '    '
         THEN IDUR = '  1';
         IF INODE3 = ' '
         THEN GO TO B2;
 B1:     OUTREC = ' ';
         ONODE1 = INODE3;
         ONODE2 = INODE2;
         ODUR = '  0';
         OTYPE = 'DUMMY';
         ODESC = 'TO ESTABLISH PRE-REQUISITES';
         WRITE FILE(OUT) FROM(OUTREC);
 B2:     OUTREC = ' ';
         ONODE1 = INODE2;
         ONODE2 = INODE1;
         ODUR = IDUR;
         OTYPE = ITYPE;
         ODESC = IDESC;
         OMAN = IMAN;
         OSTAT = ISTAT;
         IF ONODE1 = ' '
         THEN ONODE1 = '0001';
         WRITE FILE(OUT) FROM(OUTREC);
 B3:     IF INODE4 = ' '
         THEN GO TO B4;
         OUTREC = ' ';
         ONODE1 = INODE1;
         ONODE2 = INODE4;
         ODUR = '  0';
         OTYPE = 'FORCE';
         ODESC = 'COMPLETION DATE OF TASK';
         WRITE FILE(OUT) FROM(OUTREC);
 B4:
         GO TO READ;
 END:    CLOSE FILE(IN);
         CLOSE FILE(OUT);
         END;
