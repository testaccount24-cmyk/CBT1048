//STEP02       EXEC PGM=B1801
//*
//*      DELETE ALL COMMENTS FROM THE TASK TABLE
//*
//SYSPRINT     DD   SYSOUT=A
//SYSUT1       DD   DSN=&&TASKTABL,DISP=(OLD,DELETE)
//SYSUT2       DD   DSN=DAM.TASKTABL,DISP=OLD,LABEL=EXPDT=11111,
//             SPACE=(TRK,(5,5),RLSE),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=1680)
 DELETE,E,1,C'*'
//STEP99       EXEC RELEASE,JOB=DAMCONV1
//DAMCONV1 JOB (1580,5),'0013 LIST MAJOR PROJ',CLASS=N,TYPRUN=HOLD
//STEP01       EXEC PGM=B1801
//*
//*      SELECT ONLY RECORDS DESCRIBING A MAJOR ACTIVITY ( I.E.
//*       A SUB-PROJECT )
//*
//SYSPRINT     DD   SYSOUT=A
//SYSUT1       DD   DSN=DAM.TASKTABL,DISP=SHR
//SYSUT2       DD   DSN=&&TASKTABL,DISP=(,PASS),UNIT=SYSDA,
//             SPACE=(TRK,(5,5),RLSE),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=1680)
//SYSIN        DD   *
 SELECT,E,4,C'*'
//STEP02       EXEC PGM=IEBPTPCH
//*
//*      LIST THE MAJOR SUB-PROJECTS
//*
//SYSPRINT     DD   SYSOUT=A
//SYSUT1       DD   DSN=&&TASKTABL,DISP=(OLD,PASS)
//SYSUT2       DD   SYSOUT=A
//SYSIN        DD   *
     PRINT     MAXFLDS=5
     TITLE     ITEM=('MVS CONVERSION: MAJOR PROJECTS',40)
     TITLE     ITEM=(' ',1)
     RECORD    FIELD=(3,1,,10),FIELD=(3,9,,20),FIELD=(25,26,,30),      *
               FIELD=(3,73,,60),FIELD=(3,76,,66)
//STEP03       EXEC SORTD REGION=56K
//SORTLIB      DD   DSN=SYS2.SYNCSORT,DISP=SHR
