APG=07,                      AUTOMATIC PRIORITY GROUP IS 7      DEFAULT
BLDL=DE,                     IEABLD00 TABLE
CMD=DE,                      SET TOD PROMPT,SDUMP,TRACE ON      DEFAULT
CSA=2000,                    PID DEFAULT: CSA=100,              DEFAULT
DUMP=DASD,                   PLACE SVC DUMPS ON DASD DEVICES    DEFAULT
FIX=,                        NO FIX MODULES SPECIFIED
HARDCPY=(SYSLOG),            HARDCOPY LOG IS SYSTEM LOG(SYSLOG)
IPS=DE,                      SELECT IEAIPSDE INSTALL PERF SPECS FOR SRM
LNK=,                        SPECIFY NO LINKLIST MEMBER
LOGCLS=A,                         DEFAULT
LOGLMT=000500,               MUST BE 6 DIGITS, MAX WTL MESSAGES
MAXUSER=256,                 (SYS TASKS + INITS + TSOUSERS) < 256
OPI=YES,                     ALLOW OPERATOR OVERRIDE TO IEASYS00
OPT=,                        SPECIFY IEAOPTDE (SRM TUNING PARMETERS)
PAGE=(PAGE.IPOJ0101,         PLPA PAGE D/S          -- GET IT RIGHT THE
      PAGE.IPOJ0102,         COMMOM PAGE D/S             -- FIRST TIME,
      PAGE.IPOJ0103),        FIRST LOCAL(USER) PAGE D/S   -- SEE NOTE 2
PAGNUM=(1,1),                ALLOW ADDITION OF 1 PAGE D/S & 1 SWAP D/S
REAL=76,                      DEFAULT
RSU=0,                       NO RECONFIG STORAGE UNITS          DEFAULT
SMF=DE,                      SELECT SMFPRMDE, SMF PARMETERS     DEFAULT
SQA=1,                                                          DEFAULT
VAL=00,                      SELECT VATLST00
VRREGN=64,                   DEFAULT REAL-STORAGE REGION SIZE   DEFAULT
WTOBFRS=20,                  SET NUMBER OF WTO BUFFERS
WTORPLY=5                    SET NUMBER OF WTOR BUFFERS
