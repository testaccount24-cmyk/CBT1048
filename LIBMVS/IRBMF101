         CPU              /* DEFAULT   COLLECT CPU STATISTICS
         PAGING           /* DEFAULT   COLLECT PAGING STATISTICS     */
         CHAN             /* DEFAULT   COLLECT CHANNEL STATISTICS    */
         WKLD(PERIOD)     /* DEFAULT   COLLECT WORKLOAD MANAGER
                                       STATISTICS AND REPORT AT THE
                                       PERIOD LEVEL                  */
         DEVICE(NOUNITR)  /* CHOSEN    COLLECT UNIT RECORD DEVICE
                                       STATISTICS                    */
         DEVICE(NOTAPE)   /* CHOSEN    COLLECT TAPE DEVICE
                                       STATISTICS                    */
         DEVICE(DASD)     /* DEFAULT   COLLECT DIRECT ACCESS DEVICE
                                       STATISTICS                    */
         DEVICE(COMM)     /* DEFAULT   COLLECT COMMUNICATION
                                       EQUIPMENT STATISTICS          */
         DEVICE(NOGRAPH)  /* CHOSEN    COLLECT GRAPHICS DEVICE
                                       STATISTICS                    */
         DEVICE(NOCHRDR)  /* CHOSEN    COLLECT CHARACTER READER
                                       DEVICE STATISTICS             */
         NOREPORT         /* CHOSEN    DO NOT PRINT REPORTS          */
         SYSOUT(A)        /* DEFAULT   REPORT IN SYSOUT CLASS A      */
         RECORD           /* DEFAULT   SMF RECORDING                 */
         INTERVAL(60M)    /* CHOSEN    REPORT AT 60 MINUTE INTERVALS */
         CYCLE(999)       /* CHOSEN    SAMPLE AT 999 MILLISECOND
                                       INTERVALS                     */
         NOSTOP           /* CHOSEN    DO NOT STOP                   */
         OPTIONS          /* DEFAULT   ALLOW OPERATOR TO EXAMINE AND/
