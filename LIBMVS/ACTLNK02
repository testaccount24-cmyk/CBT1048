//ACTLNK02 JOB (5803,5),'0363 NUCLEUS',CLASS=N
//*
//*      RELINK NUCLEUS WITH IGC254, SMFEXEXT, TSLIHEXT
//*
//STEP01       EXEC LKED,REGION=300K,
//      PARM='SYSLMOD,IEWL,LIST,XREF,DC,NCAL,SCTR,LET,SIZE=(256K,64K)'
//SYSLMOD      DD   DSN=SYS2.TESTLIB,DISP=SHR
//NUCLEUS DD   DSN=SYS1.NUCLEUS,DISP=SHR,UNIT=3330-1,VOL=SER=IPOR20
//TESTLIB      DD   DSN=SYS2.TESTLIB,DISP=SHR
 INSERT IEAVNIP0
 INSERT IEAVFX00
 INSERT IGFPMCIH,IGFRWAC
 INSERT IGFPMPFX,IGFPMSCA,IGFPMHCA
 INSERT IGFPMRTM,IGFPMTHA,IGFPMFRS
 INSERT IGFPMMSG,IGFPMKTA,IGFPKREF
 INSERT IGFPTERM,IGFPTSIG
 INSERT IGFPXMFA,IGFPTAIM
 INSERT IGFPBUCR,IGFPEXIT
 INSERT IGFPTCON,IGFPTREC
 REPLACE IGC254
 REPLACE SMFEXEXT
 REPLACE TSLIHEXT
 INCLUDE NUCLEUS(IEANUC01)
 INCLUDE TESTLIB(IGC254)
 INCLUDE TESTLIB(SMFEXEXT)
 INCLUDE TESTLIB(TSLIHEXT)
 NAME IEANUC01(R)
