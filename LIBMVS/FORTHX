//FORTRANX JOB 02200000,'FORTRANX',MSGLEVEL=(1,1)
//*                          THE FOLLOWING JOB IS THE
//*                          PP INSTALLATION PROCEDURE FOR THE
//*                          FORTRAN IV ( H EXTENDED) COMPILER
//*                          (5734-FO3 COPYRIGHT IBM CORP 1971)
//STEP1 EXEC PROC=FORTXDEF
//*                          THIS STEP DEFINES THE TARGET LIBRARIES
//STEP2 EXEC PGM=IEBUPDTE,PARM='NEW',COND=(8,LE)
//*                          THIS STEP PLACES THE MACRO
//*                          IN A TEMPORARY DATA SET
//SYSUT2 DD DSN=&FORTMAC,UNIT=SYSDA,SPACE=(80,(200,20,5)),             *
//     DCB=SYS1.MACLIB,DISP=(NEW,PASS)
//SYSPRINT DD SYSOUT=A,DCB=*.STEP1.STEPABC.SYSPRINT
//SYSIN DD UNIT=2400,LABEL=(3,NL),VOL=SER=HXTAPE,                      *
//   DISP=(OLD,PASS),DCB=(RECFM=FB,LRECL=80,BLKSIZE=80)
//STEP3 EXEC PGM=IEUASM,COND=(8,LE),REGION=176K
//*                          THIS STEP CAUSES ALL OPTIONS TO BE
//*                          CHECKED, AND ASSEMBLES THE
//*                          OPTIONS IF THERE ARE NO ERRORS.
//SYSPRINT DD SYSOUT=A,DCB=*.STEP1.STEPABC.SYSPRINT
//SYSLIB DD DSN=&FORTMAC(FORTRANX),DISP=(OLD,DELETE)
//       DD DSN=SYS1.MACLIB,DISP=SHR
//SYSUT1 DD DSN=&FORTUT1,SPACE=(1700,(200,100)),UNIT=SYSDA,            *
//     DISP=(NEW,DELETE)
//SYSUT2 DD DSN=&FORTUT2,SPACE=(1700,(200,100)),UNIT=SYSDA,            *
//     DISP=(NEW,DELETE)
//SYSUT3 DD DSN=&FORTUT3,SPACE=(1700,(200,100)),UNIT=SYSDA,            *
//     DISP=(NEW,PASS)
//SYSPUNCH DD DSN=&FORTOBJ(OPTIONS),SPACE=(80,(300,20,5)),             *
//     UNIT=SYSDA,DISP=(NEW,PASS)
//SYSIN DD DSN=SYS1.PPOPTION(FORTRANX),DISP=SHR
//STEP4 EXEC PGM=IEWL,COND=((8,LE),(3,EQ,STEP3)),REGION=176K,
//     PARM='LIST,XREF,NCAL,OVLY,SIZE=(166K,45K)'
//*                         THIS STEP LINK EDITS THE COMPILER
//*                         INTO THE SPECIFIED OVERLAY STRUCTURE
//*                         AND PLACES THE LOAD MODULE IN THE
//*                         USER-SPECIFIED LINKAGE LIBRARY
//SYSPRINT DD SYSOUT=A,DCB=*.STEP1.STEPABC.SYSPRINT
//SYSLMOD DD DSN=*.STEP1.STEPABC.PPLINK,DISP=SHR
//SYSUT1 DD DSN=&FORTUT3,DISP=(OLD,DELETE)
//SYSLIN DD DSN=&FORTOBJ(OPTIONS),DISP=(OLD,DELETE)
//       DD DDNAME=SYSIN
//       DD UNIT=2400,LABEL=(4,NL),VOL=SER=HXTAPE,
//   DISP=(OLD,PASS),DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200)
//SYSIN  DD *
  INSERT IFEAAA,IFEAAB,IFEAAC,IFEAAE,IFEADC,IFEAIB,IFEAIO,IFEALD,IFEAPD
  INSERT IFEAPT,IFEAAD
 OVERLAY PHASE
  INSERT IFEANT
 OVERLAY PHASE
  INSERT IFECAA,IFECCR
 OVERLAY DICT
  INSERT IHOFCONI,IHOFTEN,IHOFQTEN,IHOQCONI,IFECDP,IFECDT,IFECGC
  INSERT IFECGW,IFECLT,IFECPX,IFECSR,IFECTN,IFEDCL,IFEDIO,IFEXRS
 OVERLAY EXNONEX
  INSERT IFECSP
 OVERLAY EXNONEX
  INSERT IFEBUF,IFECAR,IFECDI,IFECDO,IFECGO,IFECIO
 OVERLAY DICT
  INSERT IFEGFM,IFEGOS,IFEGST
 OVERLAY PHASE
  INSERT IFEXRF
 OVERLAY PHASE
  INSERT IFEJAB
 OVERLAY COMMON
  INSERT IFEJAC,IFEJAL,IFEJAN,IFEJBF,IFEJCM,IFEJCP,IFEJDF,IFEJFI,IFEJFU
  INSERT IFEJGR,IFEJIM,IFEKCN,IFEKOP,IFEKPA,IFEKRE,IFEKSA,IFEKSM,IFEKST
  INSERT IFEKUN,IFELAB,IFELER,IFELGN,IFELMA,IFELOK,IFELRG,IFELTB
 OVERLAY COMMON
  INSERT IFEGAA,IFEGCR,IFEGCZ,IFEGDA,IFEGDF,IFEGDT,IFEGEV,IFEGNL,IFEGSD
 OVERLAY COMMON
  INSERT IFEPGK,IFEPLP,IFEPLS,IFEPOP
 OVERLAY LOOPS
  INSERT IFEPBD,IFEPBL,IFEPBT,IFEPBZ
 OVERLAY LOOPS
  INSERT IFEQBM,IFEQCF,IFEQCL,IFEQCM,IFEQKO,IFEQMT,IFEQPF,IFEQSM,IFEQSR
  INSERT IFEQTL,IFEQWT,IFEQXM,IFEQXS,IFEQXX
 OVERLAY LOOPS
  INSERT IFERBK,IFERBP,IFERFL,IFERFP,IFERFR,IFERGB,IFERLA,IFERRG,IFERSX
  INSERT IFERTB
 OVERLAY LOOPS
  INSERT IFERLL,IFERSL,IFERSS
 OVERLAY COMMON
  INSERT IFESBS
  INSERT IFETAA,IFETCG,IFETLS,IFETPK
  INSERT IFETAB,IFETEN,IFETEP,IFETIO,IFETIS,IFETPR,IFETRN,IFETSR,IFEUEN
  INSERT IFEVCA,IFEVCC,IFEVFN,IFEWCN
  INSERT IFETLM,IFETMP,IFEUED
 OVERLAY PHASE
  INSERT IFEYER,IFEZER
 ENTRY IFEAAB
//STEP5 EXEC PGM=IEWL,COND=((8,LE),(2,EQ,STEP3)),REGION=176K,
//     PARM='LIST,XREF,NCAL,SIZE=(166K,45K)'
//*                         THIS STEP LINK EDITS THE COMPILER
//*                         WITHOUT ANY OVERLAY STRUCTURE
//*                         AND PLACES THE LOAD MODULE IN THE
//*                         USER-SPECIFIED LINKAGE LIBRARY
//SYSPRINT DD SYSOUT=A,DCB=*.STEP1.STEPABC.SYSPRINT
//SYSLMOD DD DSN=*.STEP1.STEPABC.PPLINK,DISP=OLD
//SYSUT1 DD DSN=&FORTUT3,DISP=(OLD,DELETE)
//SYSLIN DD DSN=&FORTOBJ(OPTIONS),DISP=(OLD,DELETE)
//       DD DDNAME=SYSIN
//       DD UNIT=2400,LABEL=(4,NL),VOL=SER=HXTAPE,
//   DISP=(OLD,PASS),DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200)
//SYSIN  DD *
 ENTRY IFEAAB
//STEP6 EXEC PGM=IEBUPDTE,PARM=NEW,COND=((8,LE),(3,EQ,STEP3))
//*                          THIS STEP ADDS CATALOGUED PROCEDURES
//*                          FOR AN OVERLAYED COMPILER
//*                          TO THE USER-SPECIFIED PROCEDURE LIBRARY
//SYSPRINT DD SYSOUT=A,DCB=*.STEP1.STEPABC.SYSPRINT
//SYSUT2 DD DSN=*.STEP1.STEPABC.PPPROC,DISP=OLD
//SYSIN DD UNIT=2400,LABEL=(5,NL),VOL=SER=HXTAPE,
//   DISP=(OLD,KEEP),DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200)
//STEP7 EXEC PGM=IEBUPDTE,PARM=NEW,COND=((8,LE),(2,EQ,STEP3))
//*                          THIS STEP ADDS CATALOGUED PROCEDURES
//*                          FOR AN NON-OVERLAYED COMPILER
//*                          TO THE USER-SPECIFIED PROCEDURE LIBRARY
//SYSPRINT DD SYSOUT=A,DCB=*.STEP1.STEPABC.SYSPRINT
//SYSUT2 DD DSN=*.STEP1.STEPABC.PPPROC,DISP=OLD
//SYSIN DD UNIT=2400,LABEL=(6,NL),VOL=SER=HXTAPE,
//   DISP=(OLD,KEEP),DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200)
//STEP8 EXEC PGM=IEBGENER,COND=(8,LE)
//*                          THIS STEP PUNCHES OUT A SAMPLE PROGRAM
//*                          TO TEST THE SUCCESS OF THE
//*                          INSTALLATION PROCEDURE.
//SYSPRINT DD SYSOUT=A
//SYSIN DD DUMMY
//SYSUT2 DD SYSOUT=B,DCB=BLKSIZE=80
//SYSUT1 DD UNIT=2400,LABEL=(7,NL),VOL=SER=HXTAPE,
//   DISP=(OLD,KEEP),DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200)
