         CPU             /* DEFAULT -  COLLECT CPU STATISTICS       */
         PAGING          /* DEFAULT -  COLLECT PAGING STATISTICS    */
         CHAN            /* DEFAULT -  COLLECT CHANNEL STATISTICS   */
         WKLD(PERIOD)    /* CHOSEN  -  COLLECT WORKLOAD MANAGER
                                       STATISTICS AND REPORT AT THE
                                       PERIOD LEVEL                  */
         DEVICE(NOUNITR) /* CHOSEN  -  DO NOT COLLECT UNIT RECORD
                                       DEVICE STATISTICS             */
         DEVICE(NOTAPE)  /* DEFAULT -  COLLECT TAPE DEVICE
                                       STATISTICS                    */
         DEVICE(DASD)    /* DEFAULT -  COLLECT DIRECT ACCESS DEVICE
                                       STATISTICS                    */
         DEVICE(COMM)    /* DEFAULT -  COLLECT COMMUNICATION
                                       EQUIPMENT STATISTICS          */
         DEVICE(NOGRAPH) /* CHOSEN  -  DO NOT COLLECT GRAPHICS DEVICE
                                       STATISTICS                    */
         DEVICE(NOCHRDR) /* CHOSEN  -  DO NOT COLLECT CHARACTER READER
                                       DEVICE STATISTICS             */
         PAGESP          /* CHOSEN  -  PAGE SWAP DATASET REPORT IS
                                           RECORDING                 */
         REPORT(DEFER)   /* DEFAULT -  PRINT REPORTS AFTER RMF
                                       TERMINATION                   */
         SYSOUT(A)       /* DEFAULT -  NOT USED  - NO REPORT         */
         RECORD          /* CHOSEN  -  SMF RECORDING                 */
         INTERVAL(15M)   /* CHOSEN  -  REPORT AT 15 MINUTE INTERVALS
                                       FOR HIGH CONFIDENCE FACTOR    */
         CYCLE(250)      /* CHOSEN  -  SAMPLE AT 250 MILLISECOND
                                       INTERVALS                     */
         STOP(60M)       /* CHOSEN  -  STOP AFTER 60 MINUTES         */
         TRACE(RCVUICA,END)  /* TRACE RESOURCE MONITOR UCI AVERAGE   */
         TRACE(MCVSTCRI,AVG) /* TRACE HIGHEST SYSTEM UIC             */
         TRACE(RCVCPUA,END)  /* TRACE RESOURCE MONITOR CPU AVG*16    */
         TRACE(RCVAVQC,END)  /* TRACE RESOURCE MONITOR AVQ LOW COUNT */
         TRACE(RCVASMQA,END) /* TRACE RESOURCE MONITOR ASMQ AVG      */
         TRACE(RCVPTR,END)   /* TRACE RESOURCE MONITOR PAGEFAULT AVG */
         TRACE(RCVTOTDF,END) /* TRACE AVG LOGICAL CHAN UTILIZATION   */
         TRACE(DMDTMPLT,END) /* TRACE DOMAIN TABLE TARGET MPL        */
         TRACE(DMDTCMPL,AVG) /* TRACE DOMAIM TABLE CURRENT MPL       */
         TRACE(DMDTINCU,AVG) /* TRACE USERS PER DOMAIN IN STORAGE    */
         TRACE(DMDTOUTU,AVG) /* TRACE USERS PER DOMAIN OUT OF STG    */
         TRACE(DMDTRUA,END)  /* TRACE AVERAGE NUMBER OF READY USERS  */
         OPTIONS         /* DEFAULT -  ALLOW OPERATOR TO EXAMINE AND/
                                       OR TO CHANGE THE RMF OPTIONS */
