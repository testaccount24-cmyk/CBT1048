//SYSUT1   DD DSN=&CBC4UT1,SPACE=(1700,(200,100)),UNIT=SYSDA
//SYSUT2   DD DSN=&CBC4UT2,SPACE=(1700,(200,100)),UNIT=SYSDA
//SYSUT3   DD DSN=&CBC4UT3,SPACE=(1700,(200,100)),UNIT=SYSDA,
