         CPU             /* DEFAULT -   COLLECT CPU STATISTICS       */
         PAGING          /* DEFAULT -   COLLECT PAGING STATISTICS    */
         CHAN            /* DEFAULT -   COLLECT CHANNEL STATISTICS   */
         WKLD(PERIOD)    /* CHOSEN  -   COLLECT WORKLOAD MANAGER
                                       STATISTICS AND REPORT AT THE
                                       PERIOD LEVEL                  */
         DEVICE(NOUNITR) /* CHOSEN  -  DO NOT COLLECT UNIT RECORD
                                       DEVICE STATISTICS             */
         DEVICE(NOTAPE)  /* DEFAULT -  COLLECT TAPE DEVICE
                                       STATISTICS                    */
         DEVICE(DASD)    /* DEFAULT -  COLLECT DIRECT ACCESS DEVICE
                                       STATISTICS                    */
         DEVICE(COMM)    /* CHOSEN  -  COLLECT COMMUNICATION
                                       EQUIPMENT STATISTICS          */
         DEVICE(NOGRAPH) /* CHOSEN  -  DO NOT COLLECT GRAPHICS DEVICE
                                       STATISTICS                    */
         DEVICE(NOCHRDR) /* CHOSEN  -  DO NOT COLLECT CHARACTER READER
                                       DEVICE STATISTICS             */
         NOTRACE         /* DEFAULT -  ASM,RSM,SRM FIELD TRACE IS
                                       NOT RECORDING                 */
         PAGESP          /* CHOSEN  -  PAGE SWAP DATASET REPORT IS
                                           RECORDING                 */
         REPORT(DEFER)   /* DEFAULT -  PRINT REPORTS AFTER RMF
                                       TERMINATION                   */
         SYSOUT(A)       /* DEFAULT -  REPORT IN SYSOUT CLASS A      */
         RECORD          /* CHOSEN  -  SMF RECORDING                 */
         INTERVAL(15M)   /* DEFAULT -  REPORT AT 15 MINUTE INTERVALS */
         CYCLE(250)      /* CHOSEN  -  SAMPLE AT 250 MILLISECOND
                                       INTERVALS                     */
         STOP(15M)       /* DEFAULT -  STOP IN 15 MINUTES            */
         OPTIONS         /* DEFAULT -  ALLOW OPERATOR TO EXAMINE AND/
                                       OR TO CHANGE THE RMF OPTIONS */
